{{#*inline "content-block"}}
<div class="svcs">
  <div class="svcsHeader">
    <h1 class="svcsHeader__heading">Nuestros Servicios</h1>
  </div>

  <div class="svcsPlans">
    <div class="svcsPlans__heading">Planes</div>
    <div class="svcsCard svcsCard--ent">
      <div class="svcsCard__bg svcsCard__bg--ent"></div>
      <h3 class="svcsCard__heading">Emprendedor</h3>
      <div class="svcsCard__content svcsCard__content--ent">
        <h4 class="heading">Emprendedor</h4>
        <ul class="benefits">
          <li class="benefit">Creacion de logotipo</li>
          <li class="benefit">Pagina de FB e IG</li>
          <li class="benefit">Posicionamiento y reconociemto de marca</li>
          <li class="benefit">3 Publicaciones semanales</li>
        </ul>
        <button class="cta cta--ent" type="button" data-plan-name="emprendedor">$2,499</button>
      </div>
    </div>
    <div class="svcsCard svcsCard--emp">
      <div class="svcsCard__bg svcsCard__bg--emp"></div>
      <h3 class="svcsCard__heading">Empresario</h3>
      <div class="svcsCard__content svcsCard__content--emp">
        <h4 class="heading">Empresario</h4>
        <ul class="benefits">
          <li class="benefit">Creacion de logotipo</li>
          <li class="benefit">Pagina de FB e IG</li>
          <li class="benefit">Posicionamiento y reconociemto de marca</li>
          <li class="benefit">3 Publicaciones semanales</li>
          <li class="benefit">GIFS</li>
          <li class="benefit">Diseño para tarjetas de presentación</li>
          <li class="benefit">Mensajería automática</li>
        </ul>
        <button class="cta cta--emp" type="button" data-plan-name="empresario">$4,999</button>
      </div>
    </div>
    <div class="svcsCard svcsCard--mag">
      <div class="svcsCard__bg svcsCard__bg--mag"></div>
      <h3 class="svcsCard__heading">Magnate</h3>
      <div class="svcsCard__content svcsCard__content--mag">
        <h4 class="heading">Magnate</h4>
        <ul class="benefits">
          <li class="benefit">Creacion de logotipo</li>
